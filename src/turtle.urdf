<?xml version="1.0"?>
<robot name="turtle">
    <link name="base_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <geometry>
                <box size="5.0 1.0 1.0"/>
            </geometry>
        </visual>
    </link>
    <link name="left_front_leg_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <geometry>
                <box size="1.0 2.0 1.0"/>
            </geometry>
        </visual>
    </link>
    <link name="right_front_leg_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <geometry>
                <box size="1.0 2.0 1.0"/>
            </geometry>
        </visual>
    </link>
    <link name="left_rear_leg_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <geometry>
                <box size="1.0 2.0 1.0"/>
            </geometry>
        </visual>
    </link>
    <link name="right_rear_leg_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <geometry>
                <box size="1.0 2.0 1.0"/>
            </geometry>
        </visual>
    </link>

    <joint name="left_front_leg_joint" type="continuous">
        <origin xyz="2.5 -0.5 -0.5" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="left_front_leg_link"/>
        <axis xyz="0 1 0"/>
    </joint>
    <joint name="right_front_leg_joint" type="continuous">
        <origin xyz="2.5 0.5 -0.5" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="right_front_leg_link"/>
        <axis xyz="0 1 0"/>
    </joint>
    <joint name="left_rear_leg_joint" type="continuous">
        <origin xyz="-2.5 -0.5 -0.5" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="left_rear_leg_link"/>
        <axis xyz="0 1 0"/>
    </joint>
    <joint name="right_rear_leg_joint" type="continuous">
        <origin xyz="-2.5 0.5 -0.5" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="right_rear_leg_link"/>
        <axis xyz="0 1 0"/>
