    <!-- Root link to four bar linkage joint. -->
    <joint name="j1" type="revolute">
        <parent link="l1"/>
        <child link="l2"/>
        <axis xyz="0 0 1"/>
        <dynamics damping="500"/>
        <limit effort="1000.0" lower="0" upper="0" velocity="0.5"/>
        <origin rpy="0 0 1.57079632679" xyz="0.275 0 0"/>
    </joint>
    <joint name="j2" type="revolute">
        <parent link="l2"/>
        <child link="l2l3"/>
